stages = [
    'зигота',
    'эмбрион',
    'плод',
    'новорожденный',
    'младенец',
    'ребенок',
    'подросток',
    'взрослый'
]

answers = []

for stage in stages:
    answer = input('Введите этап развития человека после ' + stage + ': ')
    answers.append(answer)

print('Этапы развития человека: ' + ' => '.join(answers))
